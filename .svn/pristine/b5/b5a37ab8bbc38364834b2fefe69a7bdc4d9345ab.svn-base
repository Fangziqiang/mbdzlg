/*清零*/
body, ul, ol, li, p, h1, h2, h3, h4, h5, h6, form, fieldset, table, td, img, div, dl, dt, dd, input { margin: 0; padding: 0; border: 0; }
body { font-size: 12px; font-family: '\5FAE\8F6F\96C5\9ED1', '\5B8B\4F53', Verdana;  background: #f6f6f6; }

ul, ol { list-style-type: none; }
select, input, img { vertical-align: middle; outline: none; }
a { text-decoration: none; }
a { blr:expression(this.onFocus=this.blur())}
a { outline: none; }
html {overflow-x:hidden;}
.clear:after { content: ""; display: block; clear: both; }
.clear{ zoom: 1; }
.fl { float: left; }
.fr { float: right; }
.st{font-family:'\65B0\5B8B\4F53';}
.yh{font-family:'\5FAE\8F6F\96C5\9ED1';}
.w{ margin-left:auto; margin-right:auto;min-width: 1000px;max-width: 1100px; overflow:hidden;}


/*内容*/
.contentBox{width:100%;/*height: 90vh;  background-color:#f8f7f7;*/ background:url(../images/bg-login_front.jpg);}
.content{ width:1000px; height:660px; margin:0 auto; position:relative;}
.content .login_banner{ position:absolute; left:-480px; bottom:0px;}

/*.reg-inner { position:absolute; top:80px; right:100px;color: #4c6269;}*/
.reg-inner {/*width: 294px;
    margin-top: 115px;float: right;*/
    
     position: absolute; 
    right: 0px;  top: 190px; 
    
   padding-top: 20px;
    margin-bottom: 10px;
    background-color: #fff;
    border: 1px solid #D4D4D4;
    z-index: 2;
    padding: 24px 22px;
    box-shadow: 0 0 3px rgba(0,0,0,.1);
    border-radius: 4px;}
.reg-inner h2 {
font-size: 24px;
    /* line-height: 60px; */
    margin-top: 4px;
    margin-bottom: 40px;
    font-weight: 400;
    color: #2f6f9f;
    border-left: 4px solid #2f6f9f;
    padding-left: 10px;
}
.reg-inner ol {padding-top: 20px; margin:0px; padding:0px;}
.reg-inner ol li{line-height: 44px;margin-bottom: 16px;position: relative;}
.reg-inner ol li span{}

.reg-inner ol li input[type="text"]{width: 223px;height: 16px;border: 1px solid #d6d6d6;border-radius: 3px;padding: 12px 30px; margin-left:0px; font-family: "微软雅黑";}
.reg-inner ol li input[type="text"]:focus,.reg-inner ol li input[type="password"]:focus{ border:1px solid #69c5dd; background-color:#e9fbfe;}
.reg-inner ol li input[type="password"]{width: 223px;height: 16px;border: 1px solid #d6d6d6;border-radius: 3px;padding: 12px 30px; margin-left:0px;font-family: "微软雅黑";}

.reg-inner ol li .icon {width: 18px; height:18px;vertical-align: middle;position: absolute;left: 8px;top: 14px;line-height: normal;}

.reg-inner ol li .refer {display: inline-block;width: 285px;line-height: 42px;text-align: center;background: #1f8dd6;color: #fff;font-size:16px;border: none;margin: 0;cursor: pointer;border-radius: 3px;font-family: "微软雅黑";}
.reg-inner ol li .refer:hover{ background-color:#369ee3;}


/*底部*/
#footer { background: transparent; position: static; }
.footer-w { width: 900px; }


