.tooltips:hover span{display:block;position:absolute;top:15px;left:15px;width:20em;border:1px solid #bbbbbb;background:#f8f8f8 ;padding: 3px; color:#333; -webkit-border-radius:3px;border-radius:3px; }
.tooltips:hover span .xiaosanjiao {background:url(../images/tooltipbg.png)  no-repeat; position:absolute; top:-10px; left:15px; width:12px; height:10px;}

